@model IEnumerable<WebApplication2.Services.EndpointStats>

<h2>Metrics</h2>
<table class="table">
    <thead>
        <tr>
            <th>Http method</th>
            <th>Path</th>
            <th>Requests count</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var stats in Model)
            {
                <tr>
                    <td>@stats.HttpMethod</td>
                    <td>@stats.RequestUrl</td>
                    <td>@stats.RequestCount</td>
                </tr>
            }
    </tbody>
</table>
